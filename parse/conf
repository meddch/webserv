server {
	listen 127.0.0.1:4422;
	server_name www.42prototype.com;
	root 42prototype;

	error_page 403 404 405 408 /default_error/500.html;
	client_max_body_size 10;

	location / {
		index index.html;
		allowed_method GET POST;
	}

	location /style {
		alias /style;
	}

	location /script {
		alias /script;
	}

	location /upload {
		alias /upload;
		allowed_method GET POST;
	}

	location /delete {
		alias /upload;
		allowed_method DELETE;
	}
}

server {
	listen 4422;
	server_name www.42intra.com;
	root /42intra;

	location / {
		allowed_method GET POST;
		autoindex off;
		index index.html;
	}

	location /redirect {
		return 301 /about;
	}

	location /about {
		alias /42intra;
		index index.html;
	}

	location /autoindex {
		autoindex on;
	}

	location /redirectwrong {
		return 301 w;
	}

	location /style {
		alias /style;
	}

	location /script {
		alias /script;
	}

	location /upload {
		alias /upload;
		allowed_method GET POST;
	}

	location /delete {
		alias /upload;
		allowed_method DELETE;
	}
}

