server {
	server_name webserv0.1;
	listen 	5500;
	root /Users/azari/Desktop/webserv/Session2;
	upload on;
	error_page 404 409 /error/404.html;

	location /{
		autoindex on;
		root /Users/azari/Desktop/webserv/Session2/;
		upload_path /Users/azari/Desktop/webserv/Session2/uplaod;

		allowed_methods GET POST DELETE;
		upload_path /Users/azari/Desktop/webserv/Session2/;
	}
	location /upload{

		root /Users/azari/Desktop/webserv/Session2;
		allowed_methods GET POST DELETE;
		autoindex on;
	}

	location /ourgit{
		autoindex off;
		return 301 https://github.com/meddch/webserv;
	}
	location .py{
		cgi_path /usr/local/bin/python3;
		allowed_methods GET POST DELETE;
	}

	location .php{
		cgi_path /usr/bin/php;
		allowed_methods GET POST DELETE;
	}
}
