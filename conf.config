server {
	server_name webserv0.1;
	listen 	5500;

	root /Users/mechane/Desktop/webserv/Session;
	index index.html;
	upload on;

	location /html{
		alias /www;
		autoindex on;
		upload_path /Users/mechane/Desktop/webserv/Samples/upload;
		return 301 index.html;
	}
	location /www{
		alias /www;
		autoindex on;
		upload_path /Users/mechane/Desktop/webserv/Samples/upload;
		return 301 index.html;
	}
	location /{
		alias /www;
		autoindex off;
		index index.html;
		upload_path /Users/mechane/Desktop/webserv/Samples/upload;
	}
}

