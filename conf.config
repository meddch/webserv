server {
	server_name webserv0.1;
	listen 	5500;
	root /Users/mechane/Desktop/webser/Session;
	upload on;
	error_page 404 409 /error/404.html;

	location /{
		autoindex on;
		root /Users/mechane/Desktop/webser/Session/;
		upload_path /Users/mechane/Desktop/webser/Session/upload;

		allowed_methods GET POST DELETE;
		upload_path /Users/mechane/Desktop/webser/Session/;
	}
	location /upload{

		upload_path /Users/mechane/Desktop/webser/Session/upload;
		root /Users/mechane/Desktop/webser/Session;
		allowed_methods GET POST DELETE;
		autoindex on;
	}

	location /redirect/mechane.html{
		autoindex off;
		return 301 https://github.com/meddch;
	}

	location /redirect/azari.html{
		autoindex off;
		return 301 https://github.com/ZaariAnas;
	}

	location .py{
		cgi_path /usr/local/bin/python3;
		allowed_methods GET POST DELETE;
	}

	location .php{
		cgi_path /usr/bin/php;
		allowed_methods GET POST DELETE;
	}

	location .js{
		cgi_path /usr/local/bin/node;
		allowed_methods GET POST DELETE;
	}
}
