server {
	server_name webserv0.1;
	listen 	5500;
	root /Users/azari/Desktop/webserv/Session;
	upload on;
	error_page 404 409 /error/404.html;

	location /{
		autoindex off;
		root /Users/azari/Desktop/webserv/Session/;
		upload_path /Users/azari/Desktop/webserv/Session/upload;
		allowed_methods GET POST DELETE;
		index welcome.html;
	}
	location /upload{

		root /Users/azari/Desktop/webserv/Session/upload;
		upload_path /Users/azari/Desktop/webserv/Session/upload;
		allowed_methods GET POST DELETE;
		autoindex on;
	}

	location /ourgit{
		return 301 https://github.com/meddch/webserv;
		autoindex off;
	}
	location .py{
		cgi_path /usr/local/bin/python3;
		allowed_methods GET POST DELETE;
	}

	location .php{
		cgi_path /usr/bin/php;
		allowed_methods GET POST DELETE;
	}
}

